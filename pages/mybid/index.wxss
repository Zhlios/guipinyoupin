/* pages/mybid/index.wxss */

.mybid-cell {
  background: white;
  padding: 30rpx;
  margin-top: 20rpx;
}

.mybid-cell-title {
  font-size: 40rpx;
  line-height: 60rpx;
  color: #333;
}
.mybid-cell-msg {
  font-size: 30rpx;
  line-height: 60rpx;
  color: #999; 
}
.mybid-cell-cancel {
  width: 100%;
  height: 90rpx;
  border-radius: 10rpx;
  background: red;
  color: white;
  font-size: 36rpx;
  line-height: 90rpx;
  font-weight: bold;
  text-align: center;
}