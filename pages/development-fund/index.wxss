.development-fund-value {
    display: flex;
    align-items: center;
    height: 100rpx;
    padding: 0 20rpx;
    border-top: 1px solid #f5f5f5;
    border-bottom: 1px solid #f5f5f5;
}

.scroll {
    height: calc(100vh - 100rpx);
}

.scroll-item {
    padding: 20rpx;
    border-bottom: 1px solid #f5f5f5;
}
.item-flex{
    display: flex;
    align-items: center;
    justify-content: space-between;
}
.item-time{
    margin-top: 20rpx;
}
